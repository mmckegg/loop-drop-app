ChunkNode {
  padding: 5px;
  flex: 1
  display: flex
  flex-direction: row

  div.options {
    flex: 1
    padding: 8px

    div.publishedUrl {
      user-select: text;
      word-wrap: normal;
      width: 300px;
      text-overflow: ellipsis;
      font-size: 8px;
      color: white;
      margin: 3px 1px;
    }

  }

  div.slot {
    flex: 3
    background: #222;
    padding: 5px;
    box-shadow: 0 0 5px black
    overflow-y: scroll
  }


}
